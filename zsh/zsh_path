path=(
	~/.cargo/bin
	~/.npm/global/bin
	~/.local/bin
	/sbin
	/bin
	/usr/sbin
	/usr/bin
	/usr/local/sbin
	/usr/local/bin:$HOME/bin
	~/env/script/*/
)


case "$(hostname)" in
  'katana')
    path=( ~/resource/app/linux	$path )
    ;;
  'home')
    path=( ~/resource/app/bsd $path )
    ;;
esac

fpath=(
	~/env/zsh/mod/* 
	$fpath
)

